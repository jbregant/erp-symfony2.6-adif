 {% extends '::base.html.twig' %}  


 
 {% block body -%}

    <div class="row">
        <div class="col-md-12">
            <div class="portlet box blue">
                <div class="portlet-title">
                    <div class="caption">Cantidad de Visitas Diarias al Portal</div>
                </div>
                <div class="portlet-body">

                    <div class="table-toolbar" style="margin-top: 3em"> 
                        <div class="" style="margin-top: -2.5em">
                            {{ form_start(form, {'attr': {'class' : 'horizontal-form','novalidate': 'novalidate'} }) }}
                            
                            {% include ':Form:fieldsErrors.html.twig' %}

                            <div class="form-body">

                                <div class="row">
                                    <div class="col-xs-12 col-sm-6 col-md-3">
                                        <div class="form-group">
                                            {{ form_label(form.fechaDesde) }}
                                            <div class="input-icon right">
                                                <i class="fa"></i>
                                                {{ form_widget(form.fechaDesde) }}
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-xs-12 col-sm-6 col-md-3">
                                        <div class="form-group">
                                            {{ form_label(form.fechaHasta) }}
                                            <div class="input-icon right">
                                                <i class="fa"></i>
                                                {{ form_widget(form.fechaHasta) }}
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-xs-12 col-sm-6 col-md-3">
                                        <div class="form-group">
                                            {{ form_label(form.chkStrict) }}
                                            <div class="input-icon right">
                                                <i class="fa"></i>
                                                {{ form_widget(form.chkStrict) }}
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-xs-12 col-sm-6 col-md-3">
                                        <div class="form-group">
                                            <br>
                                            {{ form_widget(form.submit, {'attr': {'class': 'btn blue'}}) }}
                                        </div>
                                    </div>
                                </div>
                                
                                {{ form_widget(form._token) }}

                                {{ form_end(form, {"render_rest": false}) }}
                            </div>                   
                        </div>
                    </div>

                <table class="table table-bordered table-striped table-condensed flip-content dt-multiselect export-excel" id="table-visita"  dataexport-title="visita">
                <thead>
                    <tr class="replace-inputs filter">
                        <th></th>
                        <th class="not-in-filter"></th>
                        <th>Fecha</th>
                        <th>Cantidad</th>
                        <th class="not-in-filter"></th>
                    </tr>
                    <tr class="headers">
                        <th class="no-order entity_id"></th>
                        <th class="text-center table-checkbox no-order">
                            <input type="checkbox" class="group-checkable" data-set="#table-visita .checkboxes" />
                        </th>
                        <th>Fecha</th>
                        <th>Cantidad</th>
                        <th class="text-center no-order ctn_acciones">Acciones</th>
                    </tr>
                </thead>
                <tbody>
                </tbody>
            </table>
        </div>
    </div>
</div>
</div>
<div class="row">
    <div class="col-md-8 col-md-offset-2">
        <br>

        <div id="grafic-container"></div>
    </div>
</div>
{% endblock %} 

{% block javascripts %}
    {{ parent() }}
    <script src="https://code.highcharts.com/highcharts.js"></script>
    <script src="https://code.highcharts.com/modules/data.js"></script>
    <script src="https://code.highcharts.com/highcharts-more.js"></script>


    <script src="{{ asset('js/config/chart.js') }}" ></script>
    <script src="{{ asset('js/custom/portalproveedores/visita/index.js') }}" ></script>

{% endblock %}
