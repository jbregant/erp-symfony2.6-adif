{
"data": [
{% for entity in entities %}
    [
    "{{ entity.id }}",
    "{{ entity.id }}",
    {% if entity.fecha %}"{{ entity.fecha|date('d/m/Y') }}",{% endif %} 
    "{{ entity.email|default('-') }}", 
    "{{ entity.cuit_numero|default('-') }}", 
    "{{ entity.error|default('-') }}", 
    {
        "show" : " {{ path( "errorintegracion_show", { "id": entity.id }) }}"
    }
    ]{{ (loop.last ? '' : ',') }}
{% endfor %}
]}
