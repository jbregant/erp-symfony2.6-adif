{
    "data": [
    {% for entity in entities %}
        [
            "{{ entity.id }}",
            "{{ entity.id }}",
            "{{ entity.denominacion|default('-') }}", 
            "{{ entity.denominacionCorta|default('-') }}", 
            {
                "show" : " {{ path( "unidadmedida_show", { "id": entity.id }) }}", 
                "edit" : " {{ path( "unidadmedida_edit", { "id": entity.id }) }}",
                "delete" : " {{ path( "unidadmedida_delete", { "id": entity.id }) }}" 
            }
        ]{{ (loop.last ? '' : ',') }}
    {% endfor %}
]}
