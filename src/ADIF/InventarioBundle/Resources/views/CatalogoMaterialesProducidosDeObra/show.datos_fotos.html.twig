<fieldset class="form-section-fieldset">

    <legend class="form-section-legend">Im&aacute;genes</legend>

    <div class="row">
        <div class="col-md-12">

            {% for foto in entity.fotos %}

                <div class="col-md-4 imagenes">
                    <div class="col-md-12 form_field">
                        <div class="form-group">
                            <div class="col-lg-8 imgHolder">
                                <img src="{{ asset('uploads/inventario/' ~ foto.nombreFoto) }}" alt="" class="imgPreview p-10">
                                <div class="col-lg-12 label label-primary text-center">
                                    {{ foto.fechaCreacion| date("m/d/Y") }}
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

            {% endfor %}

        </div>
    </div>

</fieldset>
