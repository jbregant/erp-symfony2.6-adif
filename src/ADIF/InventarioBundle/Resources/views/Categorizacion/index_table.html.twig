{
    "data": [
        {% for entity in entities %}
            [
                "{{ entity.id }}",
                "{{ entity.id }}",
                "{{ entity.denominacion|default('-') }}", 
                {
                    "show" : " {{ path( "categorizacion_show", { "id": entity.id }) }}", 
                    "edit" : " {{ path( "categorizacion_edit", { "id": entity.id }) }}",
                    "delete" : " {{ path( "categorizacion_delete", { "id": entity.id }) }}" 
                }
            ]{{ (loop.last ? '' : ',') }}
        {% endfor %}
    ]
}
