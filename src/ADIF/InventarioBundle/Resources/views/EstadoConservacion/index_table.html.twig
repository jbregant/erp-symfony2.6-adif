{
    "data": [
        {% for entity in entities %}
            [
                "{{ entity.id }}",
                "{{ entity.id }}",
                "{{ entity.denominacion|default('-') }}", 
                "{{ entity.denominacionCorta|default('-') }}", 
                "{{ entity.habilitadoMaterialNuevo ? 'Si':'No'|default('-') }}", 
                "{{ entity.habilitadoMaterialProducido ? 'Si':'No'|default('-') }}", 
                "{{ entity.habilitadoMaterialRodante ? 'Si':'No'|default('-') }}", 
                "{{ entity.habilitadoActivoLineal ? 'Si':'No'|default('-') }}", 
                {
                    "show" : " {{ path( "EstadoConservacion_show", { "id": entity.id }) }}", 
                    "edit" : " {{ path( "EstadoConservacion_edit", { "id": entity.id }) }}",
                    "delete" : " {{ path( "EstadoConservacion_delete", { "id": entity.id }) }}" 
                }
            ]{{ (loop.last ? '' : ',') }}
        {% endfor %}
    ]
}
