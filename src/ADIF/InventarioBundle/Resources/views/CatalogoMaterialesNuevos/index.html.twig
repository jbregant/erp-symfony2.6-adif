 {% extends '::base.html.twig' %}



{% block body -%}

<div class="row">
    <div class="col-md-12">
        <div class="portlet box blue">
            <div class="portlet-title">
                <div class="caption"> Listado de Material Nuevo</div>
            </div>
            <div class="portlet-body">
                <div class="table-toolbar">
                {% if is_granted("ROLE_INVENTARIO_MATERIAL_NUEVO_MODIFICAR") %}
                    <div class="btn-group">
                        <a class="btn btn-sm green tooltips" data-placement="right" data-original-title="Crear material nuevo" href=" {{ path( 'catalogomaterialesnuevos_new') }} ">
                            Crear nuevo <i class="fa fa-plus"></i>
                        </a>
                    </div>
                {% endif %}
                </div>
                <table
                    class="table table-bordered table-striped table-condensed flip-content dt-multiselect export-excel"
                    id="table-catalogomaterialesnuevos"
                    dataexport-title="materialesnuevos">
                    <thead>
                        <tr class="replace-inputs filter">
                            <th></th>
                            <th class="not-in-filter"></th>
                                                        <th>NUM</th>
                                                        <th>Grupo de Material</th>
                                                        <th>Descripción</th>
                                                        <th>Medidas</th>
                                                        <th>Peso</th>
                                                        <th>Volumen</th>
                                                        <th>Un. de Medida</th>
                                                        <th>Valor</th>
                                                        <th>Tipovalor</th>
                                                        <th>Fabricante</th>
                                                        <th>Observacion</th>
                                                        <th>Estado</th>
                                                        <th class="not-in-filter"></th>
                        </tr>
                        <tr class="headers">
                            <th class="no-order entity_id"></th>
                            <th class="text-center table-checkbox no-order">
                                <input type="checkbox" class="group-checkable" data-set="#table-catalogomaterialesnuevos .checkboxes" />
                            </th>
                                                        <th>NUM</th>
                                                        <th>Grupo de Material</th>
                                                        <th>Descripción</th>
                                                        <th>Medidas</th>
                                                        <th>Peso</th>
                                                        <th>Volumen</th>
                                                        <th>Un. de Medida</th>
                                                        <th>Valor</th>
                                                        <th>Tipovalor</th>
                                                        <th>Fabricante</th>
                                                        <th>Observacion</th>
                                                        <th>Estado</th>
                                                        <th class="text-center no-order ctn_acciones">Acciones</th>
                        </tr>
                    </thead>
                    <tbody>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('js/custom/inventario/catalogomaterialesnuevos/index.js') }}" ></script>
{% endblock %}
