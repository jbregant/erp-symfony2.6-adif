{
"data": [
{% for entity in entities %}
[
"{{ entity.id }}",
"{{ entity.id }}",
    {% autoescape  %}
    "{{ entity.num|default('-') }}",
    "{{ entity.grupoMaterial|default('-') }}",
    "{{ entity.denominacion|default('-') }}",
    "{{ entity.medida|default('-') }}",
    "{{ entity.peso|default('-') }}",
    "{{ entity.volumen|default('-') }}",
    "{{ entity.unidadMedida|default('-') }}",
    "{{ entity.valor|default('-') }}",
    "{{ entity.tipoValor|default('-') }}",
    "{{ entity.fabricante|default('-') }}",
    "{{ entity.observacion|default('-') }}",
    "{{ entity.estadoInventario|default('-') }}",
    {% endautoescape %}
{
    "show" : " {{ path( "catalogomaterialesnuevos_show", { "id": entity.id }) }}",
    {% if is_granted('ROLE_INVENTARIO_MATERIAL_NUEVO_MODIFICAR') %}
    "edit" : " {{ path( "catalogomaterialesnuevos_edit", { "id": entity.id }) }}"
    {% endif %}

}
]{{ (loop.last ? '' : ',') }}
{% endfor %}
]}
