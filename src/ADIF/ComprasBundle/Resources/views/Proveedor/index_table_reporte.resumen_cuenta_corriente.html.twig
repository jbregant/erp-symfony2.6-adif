{
"data": [
{% for entity in proveedores %}
    [
    "{{ entity.id }}",
    "{{ entity.id }}",
    "{{ entity.codigo|default('-') }}",
    "{{ entity.razonSocial|default('-') }}",
    "{{ entity.cuit|default('-') }}",
    "{{ entity.tipoContratacion|default('-') }}",
    "{{ entity.numero|default('-') }}",
    "{{ entity.cuentaContable|default('-') }}",
    "{{ entity.saldoPendientePago|money_format }}"    
    ]{{ (loop.last ? '' : ',') }}
{% endfor %}
]}                            