 {% extends '::base.html.twig' %}  

 

 {% block body -%}

<div class="row">
    <div class="col-md-12">
        <div class="portlet box blue">
            <div class="portlet-title">
                <div class="caption">RenglonRequerimiento</div>
            </div>
            <div class="portlet-body">
                <div class="table-toolbar">
                    <div class="btn-group">
                        <a class="btn btn-sm green tooltips" data-placement="right" data-original-title="Crear renglonrequerimiento" href=" {{ path( 'renglonrequerimiento_new') }} ">
                            Crear renglonrequerimiento <i class="fa fa-plus"></i>
                        </a>
                    </div>
                </div>
                <table 
                    class="table table-bordered table-striped table-condensed flip-content datatable dt-multiselect export-excel" 
                    id="table-renglonrequerimiento"
                    dataexport-title="renglonrequerimiento">
                    <thead>
                        <tr class="replace-inputs filter">
                            <th class="entity_id"></th>
                            <th class="not-in-filter"></th>                            
                            <th>Cantidad</th>>                            
                            <th class="not-in-filter"></th>
                        </tr>
                        <tr class="headers">
                            <th class="no-order"></th>
                            <th class="text-center table-checkbox no-order">
                                <input type="checkbox" class="group-checkable" data-set="#table-renglonrequerimiento .checkboxes" />
                            </th>                            
                            <th>Cantidad</th>                            
                            <th class="text-center no-order ctn_acciones">Acciones</th>
                        </tr>
                    </thead>
                    <tbody>
                        {% for entity in entities %}
                        <tr>
                            <td>{{ entity.id }}</td>
                            <td class="text-center">
                                <input type="checkbox" class="checkboxes" value="" />
                            </td>                            
                            <td>{{ entity.cantidad }}</td>                            
                            <td class="ctn_acciones text-center nowrap">
                                <a href=" {{ path( 'renglonrequerimiento_show', { 'id': entity.id }) }} " class="btn btn-xs blue tooltips" data-original-title="Ver detalle"><i class="fa fa-search"></i></a>
                                <a href=" {{ path( 'renglonrequerimiento_edit', { 'id': entity.id }) }} " class="btn btn-xs green tooltips" data-original-title="Editar"><i class="fa fa-pencil"></i></a>
                            </td>
                        </tr>
                        {% endfor %}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
{% endblock %} 