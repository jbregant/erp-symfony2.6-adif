{
"data": [
{% for entity in entities %}
[
"{{ entity.id }}",
"{{ entity.id }}",
    "{{ entity.grupo|default('-') }}", 
    "{{ entity.alicuota|default('-') ~ ' %' }}",
	{% if entity.esProveedor ==  1 %}
		"Si",
	{% else %}
		"No",
	{% endif %}
{
"show" : " {{ path( "iibb_caba_show", { "id": entity.id }) }}", 
"edit" : " {{ path( "iibb_caba_edit", { "id": entity.id }) }}", 
"borrar" : " {{ path( "iibb_caba_delete", { "id": entity.id }) }}" 
}
]{{ (loop.last ? '' : ',') }}
{% endfor %}
]}
