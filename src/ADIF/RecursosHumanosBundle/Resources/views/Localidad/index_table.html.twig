{
"data": [
{% for entity in entities %}
    [
    "{{ entity.id }}",
    "{{ entity.id }}",
    "{{ entity.nombre }}",
    "{{ entity.provincia.nombre }}", 
    {
    "show" : " {{ path( "localidades_show", { "id": entity.id }) }}", 
    "edit" : " {{ path( "localidades_edit", { "id": entity.id }) }}", 
    "delete" : " {{ path( "localidades_delete", { "id": entity.id }) }}" 
    }
    ]{{ (loop.last ? '' : ',') }}
{% endfor %}
]}                        