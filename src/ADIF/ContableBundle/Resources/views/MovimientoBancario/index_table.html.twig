{
"data": [
{% for entity in entities %}
    [
    "{{ entity.id }}",
    "{{ entity.id }}",
    "{{ entity.cuentaOrigen|default("-") }}", 
    "{{ entity.cuentaDestino|default("-") }}",
    "{{ entity.fecha|date('d/m/Y') }}",
    "{{ entity.monto|money_format }}",
    "{{ entity.detalle|default('-') }}",
    {
    "show" : " {{ path( "movimientobancario_show", { "id": entity.id }) }}",
    "edit" : " {{ path( "movimientobancario_edit", { "id": entity.id }) }}",
    "anular" : " {{ path( "movimientobancario_anular", { "id": entity.id }) }}"
    }
    ]{{ (loop.last ? '' : ',') }}
{% endfor %}
]}
