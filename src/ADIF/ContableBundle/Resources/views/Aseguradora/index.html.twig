{% extends '::base.html.twig' %}



{% block body -%}

<div class="row">
    <div class="col-md-12">
        <div class="portlet box blue">
            <div class="portlet-title">
                <div class="caption">Aseguradora</div>
            </div>
            <div class="portlet-body">
                {% if is_granted('ROLE_MENU_ADMINISTRACION_FONDOS_COMPLETO') %}
                    <div class="table-toolbar">
                        <div class="btn-group">
                            <a class="btn btn-sm green tooltips" data-placement="right" data-original-title="Crear aseguradora" href=" {{ path( 'aseguradoras_new') }} ">
                                Crear aseguradora <i class="fa fa-plus"></i>
                            </a>
                        </div>
                    </div>
                {% endif %}
                <table
                    class="table table-bordered table-striped table-condensed flip-content dt-multiselect export-excel"
                    id="table-aseguradora"
                    dataexport-title="aseguradora">
                    <thead>
                        <tr class="replace-inputs filter">
                            <th></th>
                            <th class="not-in-filter"></th>
                                                        <th>Nombre</th>
                                                        <th>Detalle</th>
                                                        <th>Activo</th>
                                                        <th class="not-in-filter"></th>
                        </tr>
                        <tr class="headers">
                            <th class="no-order entity_id"></th>
                            <th class="text-center table-checkbox no-order">
                                <input type="checkbox" class="group-checkable" data-set="#table-aseguradora .checkboxes" />
                            </th>
                                                        <th>Nombre</th>
                                                        <th>Detalle</th>
                                                        <th>Activo</th>
                                                        <th class="text-center no-order ctn_acciones">Acciones</th>
                        </tr>
                    </thead>
                    <tbody>
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    <script src="{{ asset('js/custom/contable/aseguradora/index.js') }}" ></script>
{% endblock %}
