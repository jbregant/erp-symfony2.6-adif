                                <th class="text-center table-checkbox no-order">
                                    <input type="checkbox" class="group-checkable" data-set="#table-movimientobancario .checkboxes" />
                                </th>
                                <th date export-format="date">Fecha</th>
                                <th>Referencia</th>
                                <th>Origen</th>
                                <th>Destino</th>
                                <th currency>Monto</th>
                                <th>Detalle</th>
                                <th data-type="date">Anulaci&oacute;n</th>                                   
                                <th class="text-center no-order ctn_acciones">Acciones</th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
{% endblock %} 

{% block javascripts %}

    {{ parent() }}

    <script src="{{ asset('js/custom/contable/asientocontable/editar_fecha.js') }}" ></script>

    <script src="{{ asset('js/custom/contable/movimientoministerial/index.js') }}" ></script>

{% endblock %}
