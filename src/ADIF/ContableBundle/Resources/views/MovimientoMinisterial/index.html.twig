{% extends '::base.html.twig' %}  

{% block body -%}

    <div class="row">
        <div class="col-md-12">
            <div class="portlet box blue">
                <div class="portlet-title">
                    <div class="caption">Movimientos ministeriales</div>
                </div>
                <div class="portlet-body">
                    <div class="table-toolbar">
                        <div class="btn-group">
                            <a class="btn btn-sm green tooltips" data-placement="right" data-original-title="Crear movimiento ministerial" href=" {{ path( 'movimientoministerial_new') }} ">
                                Crear movimiento ministerial <i class="fa fa-plus"></i>
                            </a>
                        </div>
                    </div>
                    <table 
                        class="table table-bordered table-striped table-condensed flip-content dt-multiselect export-excel" 
                        id="table-movimientoministerial"
                        dataexport-title="movimientoministerial">
                        <thead>
                            <tr class="replace-inputs filter">
