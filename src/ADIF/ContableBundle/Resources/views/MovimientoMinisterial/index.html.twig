                                <th class="not-in-filter"></th>
                                <th data-type="date">Fecha</th>
                                <th>Referencia</th>
                                <th>Origen</th>
                                <th>Destino</th>
                                <th>Monto</th>
                                <th>Detalle</th>
                                <th data-type="date">Anulaci&oacute;n</th>                                
                                <th class="not-in-filter"></th>
                            </tr>
                            <tr class="headers">
