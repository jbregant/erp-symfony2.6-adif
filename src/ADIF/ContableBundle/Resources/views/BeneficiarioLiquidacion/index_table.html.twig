{
"data": [
{% for entity in entities %}
    [
    "{{ entity.id }}",
    "{{ entity.id }}",
    "{{ entity.razonSocial|default('-') }}", 
    "{{ entity.cuit|default('-') }}", 
    "{{ entity.domicilio|default('-') }}", 
    "{{ entity.cuentasContables|join('<br\>') }}",
