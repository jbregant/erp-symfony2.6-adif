    "show" : " {{ path( "beneficiarios_liquidacion_show", { "id": entity.id }) }}", 
    "edit" : " {{ path( "beneficiarios_liquidacion_edit", { "id": entity.id }) }}" 
    }
    ]{{ (loop.last ? '' : ',') }}
{% endfor %}
]}
