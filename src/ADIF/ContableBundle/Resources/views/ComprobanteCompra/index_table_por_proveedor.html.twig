{
"data": [
{% for comprobante in comprobantes %}
    [
    "{{ comprobante.id }}",
    "{{ comprobante.fechaComprobante|date('d/m/Y') }}",
    "{{ comprobante.tipoComprobante ~ ' (' ~ comprobante.letraComprobante ~ ')' }}",
    "{{ comprobante.puntoVenta ~ '-' ~ comprobante.numero }}",
    "{{ comprobante.cuit ~ ' - ' ~ comprobante.razonSocial }}",
    "{{ comprobante.numeroOrdenCompra|default('-') }}",
    "{{ comprobante.importePendientePago|money_format }}",
    "{{ comprobante.total|money_format }}"
    ]{{ (loop.last ? '' : ',') }}
{% endfor %}
]}