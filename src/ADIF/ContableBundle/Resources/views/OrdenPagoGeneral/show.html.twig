{% extends '::base.html.twig' %}


{% block body -%}
    <div class="portlet box blue">
        <div class="portlet-title">
            <div class="caption">
                Detalles del egreso vario: <strong>{{ entity }}</strong>
            </div>
            <div class="tools">
                <a href="javascript:;" class="collapse">
                </a>                
            </div>
        </div>
        <div class="portlet-body">
            <div class="row">    
                <div class="col-md-6">
                    <div class="form-group">
                        <label class="control-label">Importe</label>
                        <div class="detalle_show">{{ entity.importe }}</div>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="form-group">
                        <label class="control-label">Fechaordenpago</label>
                        <div class="detalle_show">{{ entity.fechaOrdenPago|date('d/m/Y H:i:s') }}</div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    <div class="form-group">
                        <label class="control-label">Fechacontable</label>
                        <div class="detalle_show">{{ entity.fechaContable|date('d/m/Y H:i:s') }}</div>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="form-group">
                        <label class="control-label">Numeroordenpago</label>
                        <div class="detalle_show">{{ entity.numeroOrdenPago }}</div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    <div class="form-group">
                        <label class="control-label">Concepto</label>
                        <div class="detalle_show">{{ entity.concepto }}</div>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="form-group">
                        <label class="control-label">Fechaanulacion</label>
                        <div class="detalle_show">{{ entity.fechaAnulacion|date('d/m/Y') }}</div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    <div class="form-group">
                        <label class="control-label">Fechaautorizacioncontable</label>
                        <div class="detalle_show">{{ entity.fechaAutorizacionContable|date('d/m/Y') }}</div>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="form-group">
                        <label class="control-label">Numeroautorizacioncontable</label>
                        <div class="detalle_show">{{ entity.numeroAutorizacionContable }}</div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    <div class="form-group">
                        <label class="control-label">Facturaconformada</label>
                        <div class="detalle_show">{{ entity.facturaConformada }}</div>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="form-group">
                        <label class="control-label">Calculosverificados</label>
                        <div class="detalle_show">{{ entity.calculosVerificados }}</div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    <div class="form-group">
                        <label class="control-label">Impuestosverificados</label>
                        <div class="detalle_show">{{ entity.impuestosVerificados }}</div>
                    </div>
                </div>
            </div>

            <div class="form-actions">
                <button type="button" class="btn default button-back" 
                        back-url="{{ path('orden_pago_general') }}">
                    <i class="m-icon-swapleft"></i> Volver
                </button>
            </div>
        </div>
    </div>
{% endblock %}
