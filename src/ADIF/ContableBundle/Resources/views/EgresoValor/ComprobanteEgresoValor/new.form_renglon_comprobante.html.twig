<div class="row row_renglon_comprobante_nuevo" style="display: none;">
    
     <div class="col-md-4">
        <div class="form-group">
            <div class="input-icon right">
                <i class="fa"></i>
                <select id="adif_contablebundle_comprobanteegresovalor_renglonesComprobante___name___conceptoEgresoValor" 
                        sname="adif_contablebundle_comprobanteegresovalor[renglonesComprobante][__name__][conceptoEgresoValor]" 
                        class="form-control ignore"
                        required="required">
                    {% for choice_concepto in form.renglonesComprobante.vars.prototype.conceptoEgresoValor.vars.choices %}
                        <option value="{{choice_concepto.data.id}}">{{choice_concepto.data.denominacion}}</option>
                    {% endfor %}
                </select>
            </div>
        </div>
    </div>
                
    <div class="col-md-3">
        <div class="form-group">                                
            <div class="input-icon right">
                <i class="fa"></i>
                <input type="text" id="adif_contablebundle_comprobanteegresovalor_renglonesComprobante___name___descripcion" 
                       sname="adif_contablebundle_comprobanteegresovalor[renglonesComprobante][__name__][descripcion]" 
                       class="form-control ignore" value="">
            </div>
        </div>
    </div>
    <div class="hidden">
        <div class="form-group">
            <div class="input-icon left">
                <i class="fa"></i>
                <input type="text" id="adif_contablebundle_comprobanteegresovalor_renglonesComprobante___name___cantidad" 
                       sname="adif_contablebundle_comprobanteegresovalor[renglonesComprobante][__name__][cantidad]" 
                       required="required" class="form-control currency ignore hidden" value="">
            </div>
        </div>
    </div>
    <div class="col-md-2">
        <div class="form-group">
            <div class="input-icon left">
                <i class="fa"></i>
                <input type="text" id="adif_contablebundle_comprobanteegresovalor_renglonesComprobante___name___precioUnitario" 
                       sname="adif_contablebundle_comprobanteegresovalor[renglonesComprobante][__name__][precioUnitario]" 
                       required="required" class="form-control currency ignore" value="">
            </div>
        </div>
    </div>   
    <div class="hidden">
        <div class="form-group">
            <div class="input-icon right">
                <i class="fa"></i>
                <input type="text" id="adif_contablebundle_comprobanteegresovalor_renglonesComprobante___name___neto" 
                       sname="adif_contablebundle_comprobanteegresovalor[renglonesComprobante][__name__][montoNeto]" 
                       required="required" class="form-control currency ignore hidden" value="">
            </div>
        </div>
    </div>
    <div class="col-md-1">
        <div class="form-group">
            <div class="input-icon right">
                <i class="fa"></i>
                <select id="adif_contablebundle_comprobanteegresovalor_renglonesComprobante___name___alicuotaIva" 
                        sname="adif_contablebundle_comprobanteegresovalor[renglonesComprobante][__name__][alicuotaIva]" 
                        class="form-control ignore"
                        required="required">
                    {% for choice_alicuota_iva in form.renglonesComprobante.vars.prototype.alicuotaIva.vars.choices %}
                        <option value="{{choice_alicuota_iva.data.id}}">{{choice_alicuota_iva.data.valor}}</option>
                    {% endfor %}
                </select>
            </div>
        </div>
    </div>
    <div class="col-md-1">
        <div class="form-group">
            <div class="input-icon right">
                <i class="fa"></i>
                <input type="text" id="adif_contablebundle_comprobanteegresovalor_renglonesComprobante___name___montoIva" 
                       sname="adif_contablebundle_comprobanteegresovalor[renglonesComprobante][__name__][montoIva]" 
                       required="required" class="form-control currency ignore" value="">
            </div>
        </div>
    </div>    
    <div class="col-md-1">
        <div class="form-group">
            <button class="btn btn-sm red renglon_comprobante_borrar">
                <i class="fa fa-times"></i>
            </button>
        </div>
    </div>
</div>
