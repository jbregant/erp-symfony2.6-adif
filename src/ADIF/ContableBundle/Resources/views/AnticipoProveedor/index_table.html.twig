{
"data": [
{% for entity in entities %}
    [
    "{{ entity.id }}",
    "{{ entity.id }}",
    "{{ entity.fecha|date('d/m/Y') }}",
    "{{ entity.proveedor }}", 
    "{{ entity.detalle|default('-') }}", 
    "{{ entity.monto|money_format }}", 
    {
    "show" : " {{ path( "anticiposproveedor_show", { "id": entity.id }) }}", 
    "edit" : " {{ path( "anticiposproveedor_edit", { "tipo": entity.tipo, "id": entity.id }) }}" 
    }
    ]{{ (loop.last ? '' : ',') }}
{% endfor %}
]}
