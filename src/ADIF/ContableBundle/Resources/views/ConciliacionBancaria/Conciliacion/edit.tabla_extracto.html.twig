<div class="portlet box green-seagreen">
    <div class="portlet-title">
        <div class="caption">
            <i class="fa fa-navicon"></i>Extracto bancario
        </div>
        <div class="tools">
            <a class="collapse" href="javascript:;"></a>
        </div>
    </div>
    
    <input type="hidden" id="id_es_re_abierta" value="{{ es_re_abierta }}" />
    <div class="portlet-body">
        <div class="table-toolbar clear">            
            <div class="btn-group pull-right" id="btn_eliminar_renglon" style="display:none">
                <a class="btn btn-sm red accion-borrar tooltips" data-original-title="Eliminar movimientos seleccionados" href="">
                    <i class="fa fa-times"></i>
                    Eliminar
                </a>
            </div>
            <div class="btn-group pull-right" id="btn_asignar_conceptos" style="display:none">
                <a class="btn btn-sm blue-hoki tooltips" data-placement="right" id="asignar-concepto-conciliacion" data-original-title="Asignar concepto a movimientos seleccionados" href="">
                    <i class="fa fa-check-circle"></i>
                    Asignar concepto
                </a>
            </div>
            <div class="btn-group pull-left" id="btn_generar_comprobante" style="display:none">
                <a class="btn btn-sm green-haze tooltips" data-placement="right" id="generar-comprobante" data-original-title="Generar comprobante" href="">
                    <i class="fa fa-newspaper-o"></i>
                    Generar comprobante
                </a>
            </div>
        </div>
        <table class="table table-bordered table-striped table-condensed flip-content dt-multiselect export-excel" id="table-extracto-bancario" dataexport-title="extracto_bancario">
            <thead>
                <tr class="replace-inputs filter">
                    <th></th>
                    <th class="not-in-filter"></th>
                    <th>Fecha</th>
                    <th>Referencia</th>
                    <th>Descripci&oacute;n</th>                    
                    <th>Concepto</th>
                    <th>Debe</th>
                    <th>Haber</th>
                    <th class="not-in-filter hidden"></th>
                    <th class="not-in-filter hidden"></th>
                    <th class="not-in-filter hidden"></th>
                    <th class="not-in-filter hidden"></th>
                </tr>
                <tr class="headers">
                    <th class="no-order entity_id"></th>
                    <th class="text-center table-checkbox no-order">
                        <input type="checkbox" class="group-checkable not-checkbox-transform" data-set="#table-extracto-bancario .checkboxes" />
                    </th>
                    <th date export-format="date" class="nowrap">Fecha</th>
                    <th export-format="number" >Referencia</th>
                    <th>Descripci&oacute;n</th>                    
                    <th>Concepto</th>
                    <th currency export-format="currency" >Debe</th>
                    <th currency export-format="currency" >Haber</th>
                    <th class="hidden"></th>
                    <th class="hidden"></th>
                    <th class="hidden"></th>
                    <th class="hidden"></th>
                </tr>
            </thead>
            <tbody>
            </tbody>
        </table>
    </div>
</div>