{
"data": [
{% for entity in entities %}
    [
    "{{ entity.id }}",
    "{{ entity.id }}",
    "{{ entity.codigo|default('-') }}", 
    "{{ entity.denominacion|default('-') }}", 
    {
    "show" : " {{ path( "centrocosto_show", { "id": entity.id }) }}"
    {% if is_granted("ROLE_MENU_CONTABILIDAD_ADMINISTRAR_CENTRO_COSTO") %}
        ,"edit" : " {{ path( "centrocosto_edit", { "id": entity.id }) }}" 
        ,"delete" : " {{ path( "centrocosto_delete", { "id": entity.id }) }}" 
    {% endif %}
    }
    ]{{ (loop.last ? '' : ',') }}
{% endfor %}
]}
