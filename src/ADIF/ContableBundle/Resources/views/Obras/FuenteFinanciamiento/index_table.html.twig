{
"data": [
{% for entity in entities %}
    [
    "{{ entity.id }}",
    "{{ entity.id }}",
    "{{ entity.codigo|default('-') }}", 
    "{{ entity.nombre|escape_json|default('-') }}", 
    "{{ entity.cuentaContable|escape_json|default('-') }}", 
    "{{ entity.modificaCuentaContable ? 'Si' : 'No' }}", 
    {
    "show" : " {{ path( "fuentefinanciamiento_show", { "id": entity.id }) }}", 
    "edit" : " {{ path( "fuentefinanciamiento_edit", { "id": entity.id }) }}" 
    }
    ]{{ (loop.last ? '' : ',') }}
{% endfor %}
]}
