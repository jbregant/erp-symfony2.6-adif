{% extends '::base.html.twig' %}


{% block body -%}
    <div class="portlet box blue">
        <div class="portlet-title">
            <div class="caption">
                Detalles de la fuente de financiamiento: <strong>{{ entity }}</strong>
            </div>
            <div class="tools">
                <a href="javascript:;" class="collapse">
                </a>                
            </div>
        </div>
        <div class="portlet-body">
            <div class="row">    
                <div class="col-md-4">
                    <div class="form-group">
                        <label class="control-label">C&oacute;digo</label>
                        <div class="detalle_show">{{ entity.codigo }}</div>
                    </div>
                </div>
                <div class="col-md-8">
                    <div class="form-group">
                        <label class="control-label">Nombre</label>
                        <div class="detalle_show">{{ entity.nombre }}</div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-9">
                    <div class="form-group">
                        <label class="control-label">Cuenta contable</label>
                        <div class="detalle_show">{{ entity.cuentaContable }}</div>
                    </div>
                </div>
                <div class="col-md-3">
                    <div class="form-group">
                        <label class="control-label">Modifica cuenta contable</label>
                        <div class="detalle_show">{{ entity.modificaCuentaContable ? 'Si' : 'No' }}</div>
                    </div>
                </div>
            </div>

            <div class="form-actions">
                <button type="button" class="btn default button-back" 
                        back-url="{{ path('fuentefinanciamiento') }}">
                    <i class="m-icon-swapleft"></i> Volver
                </button>
            </div>
        </div>
    </div>
{% endblock %}
