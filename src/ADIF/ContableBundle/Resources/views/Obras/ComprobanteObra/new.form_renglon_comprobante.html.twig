<div class="row row_renglon_comprobante_nuevo" style="display: none;">

    <div class="col-md-12">
        <div class="row">
            <div class="col-md-5">
                <div class="form-group">
                    <label class="control-label required">Descripci&oacute;n</label> 
                    <div class="input-icon right">
                        <i class="fa"></i>
                        <input type="text" id="adif_contablebundle_comprobanteobra_renglonesComprobante___name___descripcion" 
                               sname="adif_contablebundle_comprobanteobra[renglonesComprobante][__name__][descripcion]" 
                               required="required" class="form-control no-editable ignore" value="">
                    </div>
                </div>
            </div>
            <div class="hidden">
                <div class="form-group">
                    <div class="input-icon left">
                        <i class="fa"></i>
                        <input type="text" id="adif_contablebundle_comprobanteobra_renglonesComprobante___name___cantidad" 
                               sname="adif_contablebundle_comprobanteobra[renglonesComprobante][__name__][cantidad]" 
                               required="required" class="form-control currency no-editable ignore hidden" value="">
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-group">
                    <label class="control-label required">Neto</label>
                    <div class="input-icon left">
                        <i class="fa"></i>
                        <input type="text" id="adif_contablebundle_comprobanteobra_renglonesComprobante___name___precioUnitario" 
                               sname="adif_contablebundle_comprobanteobra[renglonesComprobante][__name__][precioUnitario]" 
                               required="required" class="form-control currency no-editable ignore" 
                               data-digits="4" value="">
                    </div>
                </div>
            </div>   
            <div class="hidden">
                <div class="form-group">
                    <div class="input-icon right">
                        <i class="fa"></i>
                        <input type="text" id="adif_contablebundle_comprobanteobra_renglonesComprobante___name___neto" 
                               sname="adif_contablebundle_comprobanteobra[renglonesComprobante][__name__][montoNeto]" 
                               required="required" class="form-control currency no-editable ignore hidden" data-digits="4" value="">
                    </div>
                </div>
            </div>
            <div class="col-md-1">
                <div class="form-group">
                    <label class="control-label required">% IVA</label>
                    <div class="input-icon right">
                        <i class="fa"></i>
                        <select id="adif_contablebundle_comprobanteobra_renglonesComprobante___name___alicuotaIva" 
                                sname="adif_contablebundle_comprobanteobra[renglonesComprobante][__name__][alicuotaIva]" 
                                class="form-control ignore no-editable"
                                required="required">
                            {% for choice_alicuota_iva in form.renglonesComprobante.vars.prototype.alicuotaIva.vars.choices %}
                                <option value="{{choice_alicuota_iva.data.id}}">{{choice_alicuota_iva.data.valor}}</option>
                            {% endfor %}
                        </select>
                    </div>
                </div>
            </div>
            <div class="col-md-2">
                <div class="form-group">
                    <label class="control-label required">IVA</label>
                    <div class="input-icon right">
                        <i class="fa"></i>
                        <input type="text" id="adif_contablebundle_comprobanteobra_renglonesComprobante___name___montoIva" 
                               sname="adif_contablebundle_comprobanteobra[renglonesComprobante][__name__][montoIva]" 
                               required="required" class="form-control currency no-editable ignore" data-digits="4" value="">
                    </div>
                </div>
            </div>
            <div class="col-md-1">
                <div class="col-md-12">
                    <div class="form-group">
                        <button class="btn btn-sm red renglon_comprobante_borrar">
                            <i class="fa fa-times"></i>
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="col-md-12">
        <div class="row">
            <div class="col-md-3">
                <div class="form-group">
                    <label class="control-label">R&eacute;gimen retenci&oacute;n SUSS</label>
                    <div class="input-icon right">
                        <i class="fa"></i>
                        <select id="adif_contablebundle_comprobanteobra_renglonesComprobante___name___regimenRetencionSUSS" 
                                sname="adif_contablebundle_comprobanteobra[renglonesComprobante][__name__][regimenRetencionSUSS]" 
                                class="form-control ignore">
                            <option value=""> -- R&eacute;gimen retenci&oacute;n SUSS --</option>
                            {% for choiceRegimenRetencionSUSS in form.renglonesComprobante.vars.prototype.regimenRetencionSUSS.vars.choices %}
                                <option value="{{ choiceRegimenRetencionSUSS.data.id }}">{{ choiceRegimenRetencionSUSS.data.denominacionCompleta }}</option>
                            {% endfor %}
                        </select>
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-group">   
                    <div class="input-icon right">
                        <label class="control-label">R&eacute;gimen retenci&oacute;n IVA</label>
                        <i class="fa"></i>
                        <select id="adif_contablebundle_comprobanteobra_renglonesComprobante___name___regimenRetencionIVA" 
                                sname="adif_contablebundle_comprobanteobra[renglonesComprobante][__name__][regimenRetencionIVA]" 
                                class="form-control ignore">
                            <option value=""> -- R&eacute;gimen retenci&oacute;n IVA --</option>
                            {% for choiceRegimenRetencionIVA in form.renglonesComprobante.vars.prototype.regimenRetencionIVA.vars.choices %}
                                <option value="{{ choiceRegimenRetencionIVA.data.id }}">{{ choiceRegimenRetencionIVA.data.denominacionCompleta }}</option>
                            {% endfor %}
                        </select>
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-group">
                    <label class="control-label">R&eacute;gimen retenci&oacute;n IIBB</label>
                    <div class="input-icon right">
                        <i class="fa"></i>
                        <select id="adif_contablebundle_comprobanteobra_renglonesComprobante___name___regimenRetencionIIBB" 
                                sname="adif_contablebundle_comprobanteobra[renglonesComprobante][__name__][regimenRetencionIIBB]" 
                                class="form-control ignore">
                            <option value=""> -- R&eacute;gimen retenci&oacute;n IIBB --</option>
                            {% for choiceRegimenRetencionIIBB in form.renglonesComprobante.vars.prototype.regimenRetencionIIBB.vars.choices %}
                                <option value="{{ choiceRegimenRetencionIIBB.data.id }}">{{ choiceRegimenRetencionIIBB.data.denominacionCompleta }}</option>
                            {% endfor %}
                        </select>
                    </div>
                </div>
            </div>
            <div class="col-md-3">
                <div class="form-group">
                    <label class="control-label">R&eacute;gimen retenci&oacute;n Ganancias</label>
                    <div class="input-icon right">
                        <i class="fa"></i>
                        <select id="adif_contablebundle_comprobanteobra_renglonesComprobante___name___regimenRetencionGanancias" 
                                sname="adif_contablebundle_comprobanteobra[renglonesComprobante][__name__][regimenRetencionGanancias]" 
                                class="form-control ignore">
                            <option value=""> -- R&eacute;gimen retenci&oacute;n Ganancias --</option>
                            {% for choiceRegimenRetencionGanancias in form.renglonesComprobante.vars.prototype.regimenRetencionGanancias.vars.choices %}
                                <option value="{{ choiceRegimenRetencionGanancias.data.id }}">{{ choiceRegimenRetencionGanancias.data.denominacionCompleta }}</option>
                            {% endfor %}
                        </select>
                    </div>
                </div>
            </div>
            {% if edit %} 
                <div class="hidden">
                    <div class="form-group">
                        <div class="input-icon right">
                            <i class="fa"></i>
                            <input type="text" id="adif_contablebundle_comprobanteobra_renglonesComprobante___name___idRenglon" 
                                   sname="adif_contablebundle_comprobanteobra[renglonesComprobante][__name__][idRenglon]" 
                                   class="form-control currency no-editable ignore hidden" value="">
                        </div>
                    </div>
                </div>
            {% endif %} 
        </div>
    </div>
</div>
