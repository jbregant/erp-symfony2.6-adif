<div class="portlet box green-seagreen" id='div-table-cheques'>
    <div class="portlet-title">
        <div class="caption">
            Valores a depositar
        </div>
        <div class="tools">
            <a class="collapse" href="javascript:;"></a>
        </div>
    </div>
    <div class="portlet-body">
        <div class="table-toolbar clear">             
            <div class="btn-group pull-left" id="actualizar-cheques">
                <a class="btn btn-sm green tooltips" data-placement="right" data-original-title="Actualizar listado" href="">
                    <i class="fa fa-refresh"></i>
                    Actualizar listado
                </a>
            </div>
            <div class="btn-group pull-right" id="cheques_a_imputar" style="display:none">
                <a class="btn btn-sm purple accion-borrar tooltips" data-original-title="Enviar a cobranzas a imputar" href="">
                    <i class="fa fa-list-alt"></i>
                    Enviar a cobranzas a imputar
                </a>
            </div>            
            <div class="btn-group pull-right" id="eliminar-cheque" style="display:none">
                <a class="btn btn-sm red accion-borrar tooltips" data-original-title="Eliminar" href="">
                    <i class="fa fa-times"></i>
                    Eliminar
                </a>
            </div>
        </div>
        <table class="table table-bordered table-striped table-condensed flip-content dt-multiselect" id="table-cheques">
            <thead>
                <tr class="replace-inputs filter">
                    <th></th>
                    <th class="not-in-filter"></th>
                    <th>Fecha</th>                    
                    <th>Banco</th>                    
                    <th>N&uacute;mero</th>
                    <th>Observaci&oacute;n</th>                    
                    <th>Monto</th>
                    <th class="not-in-filter hidden">Id Cheque</th>                    
                </tr>
                <tr class="headers">
                    <th class="no-order entity_id"></th>
                    <th class="text-center table-checkbox no-order">
                        <input type="checkbox" class="group-checkable not-checkbox-transform" data-set="#table-cheques .checkboxes" />
                    </th>
                    <th date export-format="date" class="nowrap">Fecha</th>
                    <th>Banco</th>                    
                    <th>N&uacute;mero</th>
                    <th>Observaci&oacute;n</th>
                    <th currency >Monto</th>
                    <th class="hidden">Id Cheque</th>                        
                </tr>
            </thead>
            <tbody>
            </tbody>
        </table>
    </div>
</div>