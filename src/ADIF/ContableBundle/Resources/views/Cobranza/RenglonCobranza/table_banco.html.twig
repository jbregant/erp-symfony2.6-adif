                    <th class="text-center table-checkbox no-order">
                        <input type="checkbox" class="group-checkable not-checkbox-transform" data-set="#table-banco .checkboxes" />
                    </th>
                    <th date export-format="date" class="nowrap">Fecha</th>
                    <th>Referencia</th>                    
                    <th>C&oacute;digo/Observaci&oacute;n</th>
                    <th currency >Monto</th>
