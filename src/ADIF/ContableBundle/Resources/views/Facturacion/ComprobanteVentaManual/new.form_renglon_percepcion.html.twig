<div class="row row_renglon_percepcion_nuevo" style="display: none;">
    <div class="col-md-5">
        <div class="form-group">
            <div class="input-icon right">
                <i class="fa"></i>
                <select id="adif_contablebundle_comprobanteventa_renglonesPercepcion___name___conceptoPercepcion" 
                        sname="adif_contablebundle_comprobanteventa[renglonesPercepcion][__name__][conceptoPercepcion]" 
                        class="form-control">                                            
                    {% for choice_concepto_percepcion in form.renglonesPercepcion.vars.prototype.conceptoPercepcion.vars.choices %}
                        <option value="{{choice_concepto_percepcion.data.id}}">{{choice_concepto_percepcion.data.denominacion}}</option>
                    {% endfor %}
                </select>
            </div>
        </div>
    </div>
    <div class="col-md-2" >
        <div class="form-group">
            <div class="input-icon right">
                <i class="fa"></i>
                <select id="adif_contablebundle_comprobanteventa_renglonesPercepcion___name___jurisdiccion" 
                        sname="adif_contablebundle_comprobanteventa[renglonesPercepcion][__name__][jurisdiccion]" 
                        class="form-control">
                    <option value="">-- Jurisdicci&oacute;n --</option>
                    {% for choice_jurisdiccion in form.renglonesPercepcion.vars.prototype.jurisdiccion.vars.choices %}
                        <option value="{{choice_jurisdiccion.data.id}}">{{choice_jurisdiccion.data.denominacion}}</option>
                    {% endfor %}
                </select>
            </div>
        </div>
    </div>                        
    <div class="col-md-2">
        <div class="form-group">
            <div class="input-icon right">
                <i class="fa"></i>
                <input type="text" id="adif_contablebundle_comprobanteventa_renglonesPercepcion___name___monto" sname="adif_contablebundle_comprobanteventa[renglonesPercepcion][__name__][monto]" required="required" class="form-control currency" value="0" style="padding-right: 10px !important;">
            </div>
        </div>
    </div>
</div>  