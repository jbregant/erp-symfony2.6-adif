{
"data": [
{% for entity in entities %}
    [
    "{{ entity.id }}",
    "{{ entity.id }}",
    "{{ entity.contrato.numeroContrato|default('-') }}", 
    "{{ entity.contrato.cliente|default('-') }}", 
    "{{ entity.numeroPoliza|default('-') }}", 
    "{{ entity.aseguradora|default('-') }}", 
    "{{ entity.fechaVencimiento|date('d/m/Y') }}",
    "{{ entity.riesgoCubierto|default('-') }}", 
    "{{ entity.numeroTramiteEnvio|default('-') }}"
    ]{{ (loop.last ? '' : ',') }}
{% endfor %}
]}
